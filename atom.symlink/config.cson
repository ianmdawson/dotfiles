"*":
  core:
    disabledPackages: [
      "python-tools"
      "exception-reporting"
    ]
  editor:
    invisibles: {}
    showIndentGuide: true
    softWrapAtPreferredLineLength: true
    tabLength: 4
  "go-plus":
    formatTool: "gofmt"
    goPath: "/users/idawson/code/go"
    lintOnSave: true
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
      "text.html.basic"
    ]
  welcome:
    showOnStartup: false
